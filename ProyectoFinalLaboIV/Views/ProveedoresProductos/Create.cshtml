@model ProyectoFinalLaboIV.Models.ProveedorProducto

@{
    ViewData["Title"] = "Cargar Proveedor - Producto";
}

<h2>Cargar Proveedor - Producto</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="ProveedorId" class="control-label"></label>
                <select asp-for="ProveedorId" class ="form-control" asp-items="ViewBag.ProveedorId"></select>
            </div>
            <div class="form-group">
                <label asp-for="ProductoId" class="control-label"></label>
                <select asp-for="ProductoId" class ="form-control" asp-items="ViewBag.ProductoId"></select>
            </div>
            <hr />
            <div class="form-group">
                <input type="submit" value="Cargar" class="btn btn-success" />
                <a class="btn btn-secondary" asp-action="Index">Volver a la lista</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
